{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "index": {
      "outputs": ["src/index.ts"],
      "cache": false
    },
    "build": {
      "dependsOn": ["license", "^build"],
      "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "preview": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "constellation-tracker": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "license": {
      "cache": false,
      "dependsOn": ["index"]
    },
    "format": {
      "cache": false,
      "dependsOn": ["license"]
    },
    "bench": {
      "dependsOn": ["^build"]
    }
  }
}
