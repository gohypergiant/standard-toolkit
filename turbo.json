{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "bench": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist"]
    }
  }
}
