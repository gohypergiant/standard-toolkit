{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "index": {
      "outputs": ["src/index.ts"]
    },
    "build": {
      "dependsOn": ["index", "^build"],
      "outputs": ["dist", "build", ".next"],
      "outputLogs": "new-only"
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "preview": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage"]
    },
    "constellation-tracker": {
      "cache": false,
      "dependsOn": ["build", "^constellation-tracker"]
    },
    "lint": {},
    "bench": {
      "dependsOn": ["^build"]
    }
  }
}
