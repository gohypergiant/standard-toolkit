{
  "name": "@accelint/developer-toolkit",
  "version": "0.0.0",
  "author": "https://hypergiant.com",
  "$schema": "https://json.schemastore.org/package",
  "bugs": "https://github.com/gohypergiant/standard-toolkit/issues",
  "devDependencies": {
    "@accelint/biome-config": "workspace:*",
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/smeegl": "workspace:*",
    "@accelint/turbo-filter": "workspace:*",
    "@babel/parser": "^7.28.5",
    "@biomejs/biome": "^2.3.8",
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.29.8",
    "@ls-lint/ls-lint": "^2.3.1",
    "@swc/core": "^1.15.3",
    "lefthook": "^2.0.11",
    "publint": "^0.3.16",
    "rimraf": "^6.1.2",
    "syncpack": "^14.0.0-alpha.32",
    "taze": "^19.9.2",
    "tsx": "^4.21.0",
    "turbo": "^2.6.3",
    "zx": "^8.8.5"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "overrides": {
      "@internationalized/date": "~3.9.0",
      "@react-aria/dnd": "~3.11.2",
      "@react-aria/interactions": "~3.25.5",
      "@react-aria/overlays": "~3.29.1",
      "@react-aria/ssr": "~3.9.10",
      "@react-aria/toast": "~3.0.7",
      "@react-aria/toolbar": "~3.0.0-beta.22",
      "@react-aria/utils": "~3.30.1",
      "@react-stately/color": "~3.9.1",
      "@react-stately/data": "~3.13.2",
      "@react-stately/toast": "~3.1.2",
      "@react-stately/utils": "~3.10.8",
      "@react-types/overlays": "~3.9.0",
      "@react-types/shared": "~3.32.0",
      "react-aria-components": "~1.12.1"
    }
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/gohypergiant/standard-toolkit"
  },
  "scripts": {
    "audit:docblocks": "pnpm zx scripts/audit-docblocks.mjs",
    "bench": "pnpm turbo run bench",
    "build": "pnpm turbo run build",
    "build:design-toolkit": "pnpm turbo run build --filter=@accelint/design-toolkit",
    "build:map-toolkit": "pnpm turbo run build --filter=@accelint/map-toolkit",
    "changeset": "pnpm exec changeset",
    "changeset:release": "pnpm run build && pnpm changeset publish",
    "changeset:version": "pnpm changeset version && pnpm run constellation-tracker",
    "clean": "pnpm run clean:dist && pnpm run clean:buildinfo && pnpm run clean:turbo && pnpm run clean:cov && pnpm run clean:deps",
    "clean:buildinfo": "pnpm exec find . -name 'tsconfig.tsbuildinfo' -delete",
    "clean:cov": "pnpm -r exec rimraf coverage && pnpm rimraf coverage",
    "clean:deps": "pnpm -r exec rimraf node_modules && pnpm rimraf node_modules",
    "clean:dist": "pnpm -r exec rimraf dist",
    "clean:turbo": "pnpm -r exec rimraf .turbo && pnpm rimraf .turbo",
    "constellation-tracker": "pnpm turbo run constellation-tracker -- --regenerate",
    "context": "pnpm turbo run context",
    "deps:version-major": "pnpm taze major -r",
    "deps:version-minor": "pnpm taze minor -r",
    "deps:version-patch": "pnpm taze patch -r",
    "dev": "pnpm turbo run dev",
    "dev:filter": "pnpm turbo-filter dev",
    "format": "pnpm turbo run format",
    "format:check": "pnpm turbo run format:check",
    "format:deps": "pnpm syncpack format",
    "indexer": "pnpm turbo run index",
    "license": "pnpm turbo run license",
    "lint": "pnpm turbo run lint",
    "lint:deps": "pnpm syncpack lint",
    "lint:fs": "pnpm ls-lint",
    "lint:package": "pnpm turbo run lint:package",
    "lint:rac": "pnpm zx scripts/rac-deps.mjs",
    "pre-build": "pnpm turbo run pre-build",
    "preview": "pnpm turbo run preview",
    "preview:filter": "pnpm turbo-filter preview",
    "test": "pnpm turbo run test"
  },
  "type": "module"
}
