{
  "$schema": "https://json.schemastore.org/package",
  "name": "@accelint/icons",
  "title": "Accelint Icons",
  "description": "An open-source icon library to serve as part of the entire ecosystem of UX for Accelint.",
  "author": "https://hypergiant.com",
  "owner": "default/pathfinder",
  "keywords": [
    "design-tk",
    "tooling",
    "ui"
  ],
  "subPath": "design-toolkit/icons",
  "version": "2.1.0",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gohypergiant/standard-toolkit"
  },
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml"
  ],
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm generate:icons && pnpm license && pnpm format && pnpm tsup",
    "constellation-tracker": "constellation-tracker",
    "format": "pnpm biome check --linter-enabled=false . --write",
    "generate:icons": "svgr --config-file svgr.config.mjs -- src/svg",
    "license": "pnpm zx ../../scripts/license.mjs --files src/icons"
  },
  "dependencies": {},
  "devDependencies": {
    "@accelint/biome-config": "workspace:*",
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "@svgr/cli": "^8.1.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.11",
    "esbuild-fix-imports-plugin": "^1.0.23",
    "tsup": "8.5.0"
  },
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
