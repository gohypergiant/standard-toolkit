{
  "name": "@accelint/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:ladle": "pnpm --filter=@accelint/design-system exec ladle build -o ../../apps/docs/dist/ladle --base ladle",
    "build": "typedoc && pnpm run build:ladle",
    "serve": "http-server ./dist -p 3000"
  },
  "dependencies": {
    "http-server": "14.1.1",
    "typedoc": "0.26.11"
  }
}
