{
  "$schema": "https://json.schemastore.org/package",
  "name": "@accelint/geo",
  "title": "Accelint Geo",
  "description": "A collection of JavaScript functions for working with coordinates and geospatial data.",
  "author": "https://hypergiant.com",
  "owner": "default/pathfinder",
  "keywords": [
    "dev-tk",
    "tooling"
  ],
  "subPath": "packages/geo",
  "version": "0.2.10",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gohypergiant/standard-toolkit"
  },
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "bench": "pnpm vitest bench --run --dir src",
    "build": "pnpm tsup",
    "constellation-tracker": "constellation-tracker",
    "dev": "pnpm tsc --watch",
    "index": "pnpm zx ../../scripts/indexer.mjs packages/geo",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src",
    "test:browser": "pnpm build && node browser-testing/index.mjs"
  },
  "dependencies": {
    "@types/geodesy": "^2.2.8",
    "geodesy": "^2.4.0",
    "typescript": "5.8.3"
  },
  "devDependencies": {
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "esbuild-fix-imports-plugin": "^1.0.23",
    "playwright": "^1.56.1",
    "tsup": "8.5.0",
    "vitest": "^2.1.9"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
