{
  "$schema": "https://json.schemastore.org/package",
  "name": "@accelint/core",
  "title": "Accelint Core",
  "description": "A set of utility functions for manipulating arrays, objects, logic, and function composition in JavaScript.",
  "author": "https://hypergiant.com",
  "owner": "default/pathfinder",
  "keywords": [
    "dev-tk",
    "tooling"
  ],
  "subPath": "packages/core",
  "version": "0.4.2",
  "private": false,
  "license": "Apache-2.0",
  "repository": "gohypergiant/standard-toolkit",
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    },
    "./*": {
      "import": {
        "types": "./dist/*/index.d.ts",
        "default": "./dist/*/index.js"
      },
      "default": "./dist/*/index.js"
    }
  },
  "scripts": {
    "build": "pnpm tsup",
    "constellation-tracker": "constellation-tracker",
    "dev": "pnpm tsc --watch",
    "index": "pnpm zx ../../scripts/indexer.mjs packages/core",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src"
  },
  "devDependencies": {
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "esbuild-fix-imports-plugin": "^1.0.23",
    "tsup": "8.5.0",
    "type-fest": "^4.41.0",
    "typescript": "5.8.3",
    "uuid": "^11.1.0",
    "vitest": "^2.1.9",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "type-fest": "^4.41.0",
    "typescript": "5.8.3",
    "uuid": "^11.1.0",
    "zod": "^4.1.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
