{
  "$schema": "https://json.schemastore.org/package",
  "name": "@accelint/dataset",
  "title": "Accelint Dataset",
  "description": "A simple, consistent representation of data.",
  "author": "https://hypergiant.com",
  "owner": "default/pathfinder",
  "keywords": [
    "dev-tk",
    "tooling"
  ],
  "subPath": "packages/dataset",
  "version": "0.1.0",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gohypergiant/standard-toolkit"
  },
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./lenses": "./dist/lenses.js",
    "./types/datasets": "./dist/types/datasets.js",
    "./validation": "./dist/validation.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm tsdown",
    "constellation-tracker": "constellation-tracker",
    "dev": "pnpm tsc --watch",
    "index": "pnpm zx ../../scripts/indexer.mjs packages/dataset",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src"
  },
  "devDependencies": {
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/core": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "tsdown": "^0.17.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@accelint/core": "workspace:*",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
