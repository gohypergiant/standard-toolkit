{
  "$schema": "https://json.schemastore.org/package",
  "name": "@accelint/temporal",
  "title": "Accelint Temporal",
  "description": "A lightweight JavaScript library offering a collection of simple temporal functions for parsing, formatting, manipulating dates and times, and specialty timers.",
  "author": "https://hypergiant.com",
  "owner": "default/pathfinder",
  "keywords": [
    "dev-tk",
    "tooling"
  ],
  "subPath": "packages/temporal",
  "version": "0.1.2",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gohypergiant/standard-toolkit"
  },
  "type": "module",
  "files": [
    "./dist/**",
    "./catalog-info.yaml",
    "./CHANGELOG.md",
    "./package.json",
    "./README.md"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "bench": "pnpm vitest bench --run --dir src",
    "build": "pnpm tsup",
    "constellation-tracker": "constellation-tracker",
    "dev": "pnpm tsc --watch",
    "index": "pnpm zx ../../scripts/indexer.mjs packages/temporal",
    "lint": "pnpm biome lint",
    "test": "pnpm vitest --dir=src"
  },
  "devDependencies": {
    "@accelint/constellation-tracker": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "esbuild-fix-imports-plugin": "^1.0.23",
    "tsup": "8.5.0",
    "typescript": "5.8.3",
    "vitest": "^4.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
