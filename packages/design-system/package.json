{
  "name": "@accelint/design-system",
  "version": "0.2.0",
  "private": false,
  "license": "Apache-2.0",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": "./dist/index.js",
      "module": "./dist/index.js"
    },
    "./styles": {
      "import": {
        "default": "./dist/styles.css"
      },
      "default": "./dist/styles.css",
      "module": "./dist/styles.css"
    },
    "./vanilla": {
      "import": {
        "types": "./dist/vanilla.d.ts",
        "default": "./dist/vanilla.js"
      },
      "default": "./dist/vanilla.js",
      "module": "./dist/vanilla.js"
    }
  },
  "scripts": {
    "bench": "pnpm vitest bench --run --dir src",
    "build": "pnpm rollup --config rollup.config.js",
    "dev": "pnpm tsc --watch",
    "index": "pnpm zx ../../scripts/indexer.mjs packages/design-system --barrels",
    "lint:todo": "pnpm biome lint ./",
    "preview": "pnpm ladle serve",
    "test": "pnpm vitest --run --dir src"
  },
  "devDependencies": {
    "@accelint/biome-config": "workspace:*",
    "@accelint/typescript-config": "workspace:*",
    "@accelint/vitest-config": "workspace:*",
    "@fontsource/roboto-flex": "^5.1.0",
    "@fontsource/roboto-mono": "^5.1.0",
    "@ladle/react": "^4.1.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.11",
    "@vanilla-extract/rollup-plugin": "^1.3.10",
    "@vanilla-extract/vite-plugin": "^4.0.16",
    "@vitest/coverage-istanbul": "^2.1.3",
    "jsdom": "^25.0.1",
    "react-test-renderer": "^18.3.1",
    "rollup": "^4.24.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-node-externals": "^7.1.3",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "dependencies": {
    "@accelint/converters": "workspace:*",
    "@react-aria/collections": "3.0.0-alpha.5",
    "@react-aria/ssr": "^3.9.6",
    "@react-aria/utils": "^3.25.3",
    "@react-stately/utils": "^3.10.4",
    "@react-types/shared": "^3.25.0",
    "@vanilla-extract/css": "^1.16.0",
    "@vanilla-extract/dynamic": "^2.1.2",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-aria": "^3.35.1",
    "react-aria-components": "^1.4.1",
    "react-dom": "^18.3.1",
    "react-querybuilder": "^7.7.0",
    "react-stately": "^3.33.0",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "$schema": "https://json.schemastore.org/package",
  "author": "https://hypergiant.com",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
